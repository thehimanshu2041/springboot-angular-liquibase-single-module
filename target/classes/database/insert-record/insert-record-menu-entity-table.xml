<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

  <changeSet author="h3kumar" id="IEMET-2041-21082021-01">
    <preConditions onFail="MARK_RAN">
      <sqlCheck expectedResult="0">
        SELECT COUNT(*) FROM MENU_ENTITY;
      </sqlCheck>
    </preConditions>
    <insert tableName="MENU_ENTITY">
      <column name="MENU_ID" valueComputed="NEXTVAL('MENU_ENTITY_SEQ')"/>
      <column name="MENU_PARENT_ID" value="0"/>
      <column name="MENU_NAME" value="Login"/>
      <column name="MENU_PATH" value="/login"/>
      <column name="MENU_DESCRIPTION" value="Login"/>
      <column name="MENU_ICON" value="home"/>
      <column name="MENU_KEY" value="LOGIN"/>
      <column name="MENU_ACTIVE" value="true"/>
      <column name="MENU_IS_DELETED" value="false"/>
      <column name="MENU_IS_ADMIN" value="false"/>
      <column name="MENU_IS_VISIBLE" value="true"/>
      <column name="MENU_IS_AUTH_REQ" value="false"/>
      <column name="MENU_PRIORITY" value="0"/>
      <column name="MENU_CREATED_BY" value="ENGINE"/>
      <column name="MENU_UPDATED_BY" value="ENGINE"/>
      <column name="MENU_CREATED_DATE_TIME" valueDate="CURRENT_TIMESTAMP"/>
      <column name="MENU_MODIFIED_DATE_TIME" valueDate="CURRENT_TIMESTAMP"/>
    </insert>
  </changeSet>

</databaseChangeLog>
